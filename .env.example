# Supabase Configuration
VITE_SUPABASE_URL=your_supabase_url
VITE_SUPABASE_PUBLISHABLE_KEY=your_supabase_anon_key

# OpenRouter API Configuration (Primary AI provider)
# Get your API key from: https://openrouter.ai/keys
VITE_OPENROUTER_API_KEY=sk-or-...

# Optional fallback providers
VITE_GEMINI_API_KEY=api_key_here
VITE_OPENAI_API_KEY=sk-...
VITE_CLAUDE_API_KEY=sk-ant-...

# Stripe Configuration
# Get your API keys from: https://dashboard.stripe.com/apikeys
VITE_STRIPE_PUBLISHABLE_KEY=pk_test_xxx

# Stripe Price IDs
# Create products in Stripe Dashboard and copy the price IDs here
# Pro plan prices
VITE_STRIPE_PRO_MONTHLY_PRICE_ID=price_xxx
VITE_STRIPE_PRO_YEARLY_PRICE_ID=price_xxx

# Premium plan prices
VITE_STRIPE_PREMIUM_MONTHLY_PRICE_ID=price_xxx
VITE_STRIPE_PREMIUM_YEARLY_PRICE_ID=price_xxx
